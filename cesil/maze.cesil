*** Create maze

* Set starting point
        LOAD            1
        STORE           A7
        LOAD            4
        STORE           A8

* Loop until all cells occupied
        LOAD            2
        STORE           COMPLETED
PASS_LOOP
* Loop through all cells
        LOAD            0
        STORE           X
        STORE           Y
        STORE           CELL_NUM
CELL_LOOP
* Get current cell value
        LOAD            CELL_NUM
        STORE           INDEX
        LOAD            3
        STORE           A_READ_RET
        JUMP            ARRAY_READ
A_READ_RET_3
        LOAD            A_VAL
        STORE           THIS_CELL_VAL
* If cell already joined, skip
        JIZERO          NOT_JOINED_YET
        JUMP            NEXT_CELL
NOT_JOINED_YET

* Generate psuedo-random number
        LOAD            RAND_INTERNAL_VAL
        MULTIPLY        7
        STORE           RAND_BIGNUM
        DIVIDE          101
        MULTIPLY        101
        STORE           RAND_QUOTIENT
        LOAD            RAND_BIGNUM
        SUBTRACT        RAND_QUOTIENT
        STORE           RAND_INTERNAL_VAL
        DIVIDE          10
        MULTIPLY        10
        STORE           RAND_QUOTIENT2
        LOAD            RAND_INTERNAL_VAL
        SUBTRACT        RAND_QUOTIENT2

* Randomly choose up, left, down, right, or none
        JIZERO          JOIN_UP
        SUBTRACT        1
        JIZERO          JOIN_LEFT
        SUBTRACT        1
        JIZERO          JOIN_DOWN
        SUBTRACT        1
        JIZERO          JOIN_RIGHT
        JUMP            NEXT_CELL

* If Up...
JOIN_UP
* If on top row, skip to next cell
        LOAD            Y
        JIZERO          NEXT_CELL
* Get cell above
        LOAD            CELL_NUM
        SUBTRACT        WIDTH
        STORE           JOINING_TO
        STORE           INDEX
        LOAD            4
        STORE           A_READ_RET
        JUMP            ARRAY_READ
A_READ_RET_4

* If cell above is not occupied, skip to next cell
        LOAD            A_VAL
        JIZERO          NEXT_CELL

* Join cell above down to current cell
        ADD             2

* Store value back into cell above
        STORE           A_VAL
        LOAD            JOINING_TO
        STORE           INDEX
        LOAD            2
        STORE           A_WRITE_RET
        JUMP            ARRAY_WRITE
A_WRITE_RET_2

* Join current cell to cell above
        LOAD            THIS_CELL_VAL
        ADD             8
        JUMP            COMPLETE_CELL_JOIN

* If Left...
JOIN_LEFT
* If on left column, skip to next cell
        LOAD            X
        JIZERO          NEXT_CELL
* Get cell to left
        LOAD            CELL_NUM
        SUBTRACT        1
        STORE           JOINING_TO
        STORE           INDEX
        LOAD            5
        STORE           A_READ_RET
        JUMP            ARRAY_READ
A_READ_RET_5

* If cell to left is not occupied, skip to next cell
        LOAD            A_VAL
        JIZERO          NEXT_CELL

* Join cell at left right to current cell
        ADD             1
* Store back into cell to left
        STORE           A_VAL
        LOAD            JOINING_TO
        STORE           INDEX
        LOAD            3
        STORE           A_WRITE_RET
        JUMP            ARRAY_WRITE
A_WRITE_RET_3

* Join current cell to cell at left
        LOAD            THIS_CELL_VAL
        ADD             4
        JUMP            COMPLETE_CELL_JOIN

* If Down...
JOIN_DOWN
* If on botton row, skip to next cell
        LOAD            Y
        ADD             1
        SUBTRACT        HEIGHT
        JIZERO          NEXT_CELL
* Get cell below
        LOAD            CELL_NUM
        ADD             WIDTH
        STORE           JOINING_TO
        STORE           INDEX
        LOAD            6
        STORE           A_READ_RET
        JUMP            ARRAY_READ
A_READ_RET_6

* If cell below is not occupied, skip to next cell
        LOAD            A_VAL
        JIZERO          NEXT_CELL

* Join cell below up to current cell
        ADD             8
* Store value back into cell below
        STORE           A_VAL
        LOAD            JOINING_TO
        STORE           INDEX
        LOAD            4
        STORE           A_WRITE_RET
        JUMP            ARRAY_WRITE
A_WRITE_RET_4

* Join current cell to cell below
        LOAD            THIS_CELL_VAL
        ADD             2
        JUMP            COMPLETE_CELL_JOIN

* If right...
JOIN_RIGHT
*   If on right column, skip to next cell
        LOAD            X
        ADD             1
        SUBTRACT        WIDTH
        JIZERO          NEXT_CELL
*   Get cell to right
        LOAD            CELL_NUM
        ADD             1
        STORE           JOINING_TO
        STORE           INDEX
        LOAD            7
        STORE           A_READ_RET
        JUMP            ARRAY_READ
A_READ_RET_7

*   If cell to right is not occupied, skip to next cell
        LOAD            A_VAL
        JIZERO          NEXT_CELL

* Join cell at right left to current cell
        ADD             4
* Store value back into cell at right
        STORE           A_VAL
        LOAD            JOINING_TO
        STORE           INDEX
        LOAD            5
        STORE           A_WRITE_RET
        JUMP            ARRAY_WRITE
A_WRITE_RET_5

* Join current cell to cell right
        LOAD            THIS_CELL_VAL
        ADD             1

* Common code at end of cell joins
COMPLETE_CELL_JOIN
* Store value back into current cell
        STORE           A_VAL
        LOAD            CELL_NUM
        STORE           INDEX
        LOAD            1
        STORE           A_WRITE_RET
        JUMP            ARRAY_WRITE
A_WRITE_RET_1

* Increment joined cell count
        LOAD            COMPLETED
        ADD             1
        STORE           COMPLETED

* Go to next cell
        JUMP            NEXT_CELL


*   Go to next cell

* Next cell
NEXT_CELL
        LOAD            X
        ADD             1
        STORE           X
        SUBTRACT        WIDTH
        JINEG           NOTWRAP
        LOAD            0
        STORE           X
        LOAD            Y
        ADD             1
        STORE           Y
NOTWRAP
        LOAD            CELL_NUM
        ADD             1
        STORE           CELL_NUM

* If not at last cell, go to next cell
        SUBTRACT        TOTAL_SIZE
        JINEG           CELL_LOOP

* If not complete, do another pass
        LOAD            COMPLETED
        SUBTRACT        TOTAL_SIZE
        JINEG           PASS_LOOP

*** Print maze
PRINT_MAZE
        LOAD            0
        STORE           CELL_NUM
        STORE           Y
PRINTLOOP_Y
* Print top of cells
        LOAD            0
        STORE           X
PRINTLOOP_X
        LOAD            CELL_NUM
        STORE           INDEX
        LOAD            1
        STORE           A_READ_RET
        JUMP            ARRAY_READ
A_READ_RET_1
        LOAD            A_VAL
        SUBTRACT        8
        JINEG           HBAR
        PRINT           "+  "
        JUMP            TOP_DONE
HBAR    PRINT           "+--"
TOP_DONE
        LOAD            CELL_NUM
        ADD             1
        STORE           CELL_NUM
        LOAD            X
        ADD             1
        STORE           X
        SUBTRACT        WIDTH
        JINEG           PRINTLOOP_X
        PRINT           "+"
* Print sides of cells
        LINE
        LOAD            0
        STORE           X
        LOAD            CELL_NUM
        SUBTRACT        WIDTH
        STORE           CELL_NUM
PRINTLOOP_X2
        LOAD            CELL_NUM
        STORE           INDEX
        LOAD            2
        STORE           A_READ_RET
        JUMP            ARRAY_READ
A_READ_RET_2
        LOAD            A_VAL
        DIVIDE          4
        SUBTRACT        1
        JIZERO          VGAP
        SUBTRACT        2
        JIZERO          VGAP
* Insert entrance if X=0 and Y=0
        LOAD            X
        JIZERO          MAYBE
        JUMP            VBAR
MAYBE
        LOAD            Y
        JIZERO          VGAP
VBAR    PRINT           "|  "
        JUMP            SIDE_DONE
VGAP    PRINT           "   "
SIDE_DONE
        LOAD            CELL_NUM
        ADD             1
        STORE           CELL_NUM
        LOAD            X
        ADD             1
        STORE           X
        SUBTRACT        WIDTH
        JINEG           PRINTLOOP_X2
* Print right bar and move on to next row
        LOAD            Y
        ADD             1
        STORE           Y
        SUBTRACT        HEIGHT
        JINEG           RIGHT_SIDE
        JUMP            BASE
RIGHT_SIDE
        PRINT           "|"
        LINE
        JUMP    PRINTLOOP_Y
* Print bottom
BASE
        LINE
        LOAD            0
        STORE           X
BOTTOM_LOOP
        PRINT           "+--"
        LOAD            X
        ADD             1
        STORE           X
        SUBTRACT        WIDTH
        JINEG           BOTTOM_LOOP
        PRINT           "+"
DONE
        LINE
        HALT

